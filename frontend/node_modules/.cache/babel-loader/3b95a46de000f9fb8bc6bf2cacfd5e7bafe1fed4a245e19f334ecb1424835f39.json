{"ast":null,"code":"import { PizzaService } from '../services/pizzaService';\nexport default {\n  data() {\n    return {\n      pizzas: [],\n      pizzaForm: {\n        name: ''\n      },\n      editing: false,\n      editingId: null\n    };\n  },\n  methods: {\n    async fetchPizzas() {\n      const response = await PizzaService.getAll();\n      this.pizzas = response.data;\n    },\n    async submitPizza() {\n      if (this.editing) {\n        await PizzaService.update(this.editingId, this.pizzaForm);\n      } else {\n        await PizzaService.create(this.pizzaForm);\n      }\n      this.pizzaForm.name = '';\n      this.editing = false;\n      this.editingId = null;\n      this.fetchPizzas();\n    },\n    async deletePizza(id) {\n      await PizzaService.delete(id);\n      this.fetchPizzas();\n    },\n    async editPizza(id) {\n      const response = await PizzaService.get(id);\n      this.pizzaForm = response.data;\n      this.editing = true;\n      this.editingId = id;\n    }\n  },\n  mounted() {\n    this.fetchPizzas();\n  }\n};","map":{"version":3,"names":["PizzaService","data","pizzas","pizzaForm","name","editing","editingId","methods","fetchPizzas","response","getAll","submitPizza","update","create","deletePizza","id","delete","editPizza","get","mounted"],"sources":["D:\\Downloads\\EE\\VUEJS_DOTNET\\WebAPI_ASP.NETCore-main\\vuejs_frontend\\pizza-frontend\\src\\components\\PizzaComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h1>Pizza List</h1>\r\n      <ul>\r\n        <li v-for=\"pizza in pizzas\" :key=\"pizza.id\">\r\n          {{ pizza.name }}\r\n          <button @click=\"editPizza(pizza.id)\">Edit</button>\r\n          <button @click=\"deletePizza(pizza.id)\">Delete</button>\r\n        </li>\r\n      </ul>\r\n  \r\n      <h2>Add/Edit Pizza</h2>\r\n      <input v-model=\"pizzaForm.name\" placeholder=\"Pizza Name\" />\r\n      <button @click=\"submitPizza\">{{ editing ? 'Update' : 'Add' }} Pizza</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { PizzaService } from '../services/pizzaService';\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        pizzas: [],\r\n        pizzaForm: { name: '' },\r\n        editing: false,\r\n        editingId: null,\r\n      };\r\n    },\r\n    methods: {\r\n      async fetchPizzas() {\r\n        const response = await PizzaService.getAll();\r\n        this.pizzas = response.data;\r\n      },\r\n      async submitPizza() {\r\n        if (this.editing) {\r\n          await PizzaService.update(this.editingId, this.pizzaForm);\r\n        } else {\r\n          await PizzaService.create(this.pizzaForm);\r\n        }\r\n        this.pizzaForm.name = '';\r\n        this.editing = false;\r\n        this.editingId = null;\r\n        this.fetchPizzas();\r\n      },\r\n      async deletePizza(id) {\r\n        await PizzaService.delete(id);\r\n        this.fetchPizzas();\r\n      },\r\n      async editPizza(id) {\r\n        const response = await PizzaService.get(id);\r\n        this.pizzaForm = response.data;\r\n        this.editing = true;\r\n        this.editingId = id;\r\n      },\r\n    },\r\n    mounted() {\r\n      this.fetchPizzas();\r\n    },\r\n  };\r\n  </script>\r\n  "],"mappings":"AAkBE,SAASA,YAAW,QAAS,0BAA0B;AAEvD,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE;QAAEC,IAAI,EAAE;MAAG,CAAC;MACvBC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,MAAMC,QAAO,GAAI,MAAMT,YAAY,CAACU,MAAM,CAAC,CAAC;MAC5C,IAAI,CAACR,MAAK,GAAIO,QAAQ,CAACR,IAAI;IAC7B,CAAC;IACD,MAAMU,WAAWA,CAAA,EAAG;MAClB,IAAI,IAAI,CAACN,OAAO,EAAE;QAChB,MAAML,YAAY,CAACY,MAAM,CAAC,IAAI,CAACN,SAAS,EAAE,IAAI,CAACH,SAAS,CAAC;MAC3D,OAAO;QACL,MAAMH,YAAY,CAACa,MAAM,CAAC,IAAI,CAACV,SAAS,CAAC;MAC3C;MACA,IAAI,CAACA,SAAS,CAACC,IAAG,GAAI,EAAE;MACxB,IAAI,CAACC,OAAM,GAAI,KAAK;MACpB,IAAI,CAACC,SAAQ,GAAI,IAAI;MACrB,IAAI,CAACE,WAAW,CAAC,CAAC;IACpB,CAAC;IACD,MAAMM,WAAWA,CAACC,EAAE,EAAE;MACpB,MAAMf,YAAY,CAACgB,MAA